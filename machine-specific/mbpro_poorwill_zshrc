export GOPATH=${HOME}/code/go-path-base-dir
export PATH=${PATH}:${GOPATH}/bin

alias vim=/Applications/MacVim.app/Contents/MacOS/Vim

# 2017-01-13 put mactex binaries on PATH
export PATH=${PATH}:/usr/local/texlive/2016/bin/x86_64-darwin

# Make sure to use the correct vim.
export VISUAL=/Applications/MacVim.app/Contents/MacOS/Vim
export EDITOR='vim'

# Put anaconda on path
export PATH=~/anaconda/bin:${PATH}

# For flutter development
# export PATH='/Users/sudars/code/flutter/bin':${PATH}

# Dart pub on path
export PATH=${PATH}:"${HOME}/.pub-cache/bin"

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/sudars/anaconda/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/sudars/anaconda/etc/profile.d/conda.sh" ]; then
        . "/Users/sudars/anaconda/etc/profile.d/conda.sh"
    else
        export PATH="/Users/sudars/anaconda/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

# Generated by vg: https://github.com/GetStream/vg
command -v vg >/dev/null 2>&1 && eval "$(vg eval --shell zsh)"

# The location of the dart SDK. The `brew install` told me to note it.
# /usr/local/opt/dart/libexec

# ex: filetype=zsh
